   "source": [
    "START_YEAR = 2014\n",
    "END_YEAR = 2024\n",
    "\n",
    "\n",
    "all_players_dfs = []\n",
    "all_summaries = []\n",
    "\n",
    "\n",
    "for name in PLAYERS:\n",
    "   print(f\"\\n--- Pulling Finals shots for: {name} ---\")\n",
    "  \n",
    "   shots_df, summary_df = finals_shots_last_10_seasons(name, START_YEAR, END_YEAR)\n",
    "\n",
    "\n",
    "   # Tag who this data belongs to (useful when names collide or you change naming later)\n",
    "   shots_df = shots_df.copy()\n",
    "   shots_df[\"PLAYER_QUERY_NAME\"] = name\n",
    "  \n",
    "   # Optional: keep the summary too\n",
    "   summary_df = summary_df.copy()\n",
    "   summary_df[\"PLAYER_QUERY_NAME\"] = name\n",
    "  \n",
    "   all_players_dfs.append(shots_df)\n",
    "   all_summaries.append(summary_df)\n",
    "\n",
    "\n",
    "   # small delay between players\n",
    "   time.sleep(1.0)\n",
    "\n",
    "\n",
    "# 2) Master dataframe\n",
    "master_shots_df = pd.coccat(all_players_dfs, ignore_index=True)\n",
    "\n",
    "\n",
    "# 3) Drop duplicates (important if you re-ran pulls or overlap happens)\n",
    "# A shot is uniquely identified by (GAME_ID, GAME_EVENT_ID, PLAYER_ID)\n",
    "dedupe_cols = [\"GAME_ID\", \"GAME_EVENT_ID\", \"PLAYER_ID\"]\n",
    "dedupe_cols = [c for c in dedupe_cols if c in master_shots_df.columns]  # safety\n",
    "master_shots_df = master_shots_df.drop_duplicates(subset=dedupe_cols)\n",
    "\n",
    "\n",
    "# 4) Combine summaries (optional)\n",
    "master_summary_df = pd.concat(all_summaries, ignore_index=True)\n",
    "\n",
    "\n",
    "print(\"\\n✅ MASTER DATASET READY\")\n",
    "print(\"Total shots:\", len(master_shots_df))\n",
    "print(\"Players included:\", master_shots_df[\"PLAYER_NAME\"].nunique() if \"PLAYER_NAME\" in master_shots_df.columns else \"unknown\")\n",
    "display(master_shots_df.head())\n",
    "master_shots_df.to_parquet(\"finals_master_shots.parquet\", index=False)\n",
    "master_shots_df.to_csv(\"finals_master_shots.csv\", index=False)\n",
    "print(\"Saved: finals_master_shots.parquet and finals_master_shots.csv\")\n",
    "\n"
   ]
